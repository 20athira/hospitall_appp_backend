const express=require("express")
const hospitalmodel=require("../model/hospitalmodel")
const router=express.Router()


router.post("/add",async(req,res)=>{
    let data=req.body                //read data
    let hospital=new hospitalmodel(data)
    let result=await hospital.save()
    res.json({
        status:"sucess"
    })
    //res.send("ad")
})

router.get("/viewall",async(req,res)=>{  //display
    let data=await hospitalmodel.find()
    res.json(data)
    //res.send("vi")
})
router.post("/search",async(req,res)=>{
let input=req.body
let data=await hospitalmodel.find(input)
res.json(data)
})
module.exports=router